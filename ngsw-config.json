{
  "$schema": "https://json.schemastore.org/ngsw-config",
  "index": "index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "index.html",
          "manifest.webmanifest",
          "*.css",
          "*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "google-fonts",
      "urls": [
        "https://fonts.googleapis.com/**",
        "https://fonts.gstatic.com/**"
      ],
      "cacheConfig": {
        "maxSize": 5,
        "maxAge": "365d",
        "strategy": "cacheFirst"
      }
    },
    {
      "name": "tailwind-css",
      "urls": [
        "https://cdn.tailwindcss.com"
      ],
      "cacheConfig": {
        "maxSize": 2,
        "maxAge": "365d",
        "strategy": "cacheFirst"
      }
    }
  ]
}
